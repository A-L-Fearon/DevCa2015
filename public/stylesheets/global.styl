
html, body, div, span, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
abbr, address, cite, code,
del, dfn, em, img, ins, kbd, q, samp,
small, strong, sub, sup, var,
b, i,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, figcaption, figure, 
    footer, header, hgroup, menu, nav, section, summary,
    time, mark, audio, video
        background transparent
        border 0
        font-size 100%
        margin 0
        outline 0
        padding 0
        vertical-align baseline
        list-style none !important

body 
    line-height:1;

article, aside, details, figcaption, figure,
footer, header, hgroup, menu, nav, section
    display block

nav ul
    list-style none

blockquote, q
    quotes none

blockquote:before, blockquote:after,
q:before, q:after
    content ''
    content none

a
    background transparent
    font-size 100%
    margin 0
    padding 0
    vertical-align baseline

/* change colours to suit your needs */
ins 
    background-color #ff9
    color #000
    text-decoration none

/* change colours to suit your needs */
mark
    background-color #ff9
    color #000
    font-style italic
    font-weight bold

del
    text-decoration line-through

abbr[title], dfn[title]
    border-bottom 1px dotted
    cursor help

table
    border-collapse collapse
    border-spacing 0

/* change border colour to suit your needs */
hr
    border 0
    border-top 1px solid #ccc
    display block
    height 1px
    margin 1em 0
    padding 0

input, select 
    vertical-align middle


body,html
    margin 0
    padding 0
    height 100%
    color #5d5d54
    font-family "Roboto"
    background: -webkit-linear-gradient(90deg, #FFA17F 10%, #00223E 90%); /* Chrome 10+, Saf5.1+ */
    background:    -moz-linear-gradient(90deg, #FFA17F 10%, #00223E 90%); /* FF3.6+ */
    background:     -ms-linear-gradient(90deg, #FFA17F 10%, #00223E 90%); /* IE10 */
    background:      -o-linear-gradient(90deg, #FFA17F 10%, #00223E 90%); /* Opera 11.10+ */
    background:         linear-gradient(90deg, #FFA17F 10%, #00223E 90%); /* W3C */

//    background-color #F7F7F7


.grey
    color #848577

nav:before
    position absolute
    bottom -30px
    left -30px
    content ""
    height 0px
    width 0px
    border 30px solid transparent
    border-right 30px solid black
    z-index -1
nav 
    position relative
    z-index 2
    height 80px
    width 100%;
    display flex
    margin-top 30px
    div:nth-child(1)
        font-family Arial
        font-weight bold
        text-align left
        padding 10px
        width 400px
        display flex
        align-items center
        justify-content center
        
    ul:nth-child(2)
        display flex
        flex-grow 1
        height 100%
        width 100%
        
        li
            height 100%
            flex-grow 1
            display flex
            align-items center
            justify-content center
            border-left 1px solid rgba(154, 149, 149, 0.56)
            text-align center
        
        li:hover
            border-top 4px solid #ffa500

nav:after
    position absolute
    content ""
    top 0
    left 0
    height 100%
    width 100%
    z-index -1
    background-color #FFFFFF
    
    
header
    max-width 1200px
    width 100%
    height 450px
    margin 0 auto
    padding 20px

#header-holder
    height 100%
    width 100%
    
#map-canvas
    width 78%
    height 100%
    float left
    background-color #fbfbfb
    box-shadow -1px 1px 10px 0px rgba(101, 99, 99, 0.58)
    border-radius 4px

#specials
    float right
    width 19%
    height 100%
    background-color #fbfbfb
    box-shadow -1px 1px 10px 0px rgba(101, 99, 99, 0.58)
    border-radius 4px
    
main
    padding 20px
    width 1100px
    margin 0 auto
    height 560px
    position relative
    display flex
    justify-content space-between
    

#info
    position relative
    width 430px
    height 100%
    float left

#info-holder
    height 100%
    width 100%
    position relative
    display flex
    flex-direction column
    align-items center
    border-radius 4px
    background-color #fff
    padding-top 20px
    box-shadow -1px 1px 10px 0px rgba(101, 99, 99, 0.58)
    
    h1
        font-size 20px
    
    
#input-holder
    margin: 10px 0
    width 100%
    display flex
    justify-content center

input
    height 40px
    width 70%
    
input+div
    width 40px
    display flex
    align-items center
    justify-content center
    margin-left 20px
    border 1px solid rgba(154, 149, 149, 0.56)
    border-radius 4px
    box-shadow -0.5px 0.5px 0.5px 0px rgba(101, 99, 99, 0.58)
    background-color #fff
    transition-property background-color
    transition-duration 0.2s

input+div:hover
    background-color  #72b544
    cursor pointer
    
input+div:active
    background-color  #64864c
    box-shadow 0px 0px 0px 0px #fff

#list
    position absolute
    bottom 0px
    height 450px
    left 50%
    transform translate(-50%,0)
    width 100%
    
    ul
        list-style none
        margin-top 20px
        margin-left 20px
        overflow-y scroll
        max-height 370px
        
        li
            font-size 20px
            margin-bottom 20px
            height 30px
            line-height 30px
            
            div
                float left
            input
                width 40px
                height 30px
                float right
 
#list-header:before
    position absolute
    content ""
    height 0px
    width 0px
    bottom -18px
    left 10%
    border 10px solid transparent
    border-top 10px solid #60A531

    
#list-header
    position relative
    height 60px
    background-color #60A531
    width 100%
    text-align left
    color #fff
    font-family "Roboto"
    font-weight bold
    font-size 20px
    line-height 60px
    
    span
        padding-left 30px
        
    div:nth-child(2)
        position absolute
        right 5px
        top 50%
        transform translate(0,-50%)
        width 40px
        height 40px
        display flex
        align-items center
        justify-content center
        border 1px solid rgba(154, 149, 149, 0.56)
        border-radius 4px
        box-shadow -0.5px 0.5px 0.5px 0px rgba(101, 99, 99, 0.58)
        background-color #fff
        transition-property background-color,color
        transition-duration 0.2s
        color #d0d0d0
        
        span
            padding 0px


    div:nth-child(2):hover
        color #60A531
        cursor pointer

    div:nth-child(2):active
        background-color  #64864c
        box-shadow 0px 0px 0px 0px #fff

        
    
#results-holder
    position relative
    height 100%
    width 640px
    padding-top 20px

#results
    position absolute
    top 50%
    width 100%
    height 450px
    transform translate(0,-50%)
    background-color #fbfbfb
    box-shadow -1px 1px 10px 0px rgba(101, 99, 99, 0.58)
    border-radius 4px
    
    ul
        height 100%
        width 100%
        display flex
        align-items center
        justify-content space-between
        list-style none
        
        li
            width 29%
            height 100px
#budgetList
    position absolute
    top 50%
    left 50%
    transform translate(-50%,-50%)
    height 300px
    width 300px
    text-align left
    
    li
        height 30px
        